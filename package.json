{
  "name": "@juji/zl-fetch",
  "version": "6.0.4",
  "description": "A library that makes the Fetch API easy to use",
  "type": "module",
  "sideEffects": false,
  "files": [
    "src"
  ],
  "exports": "./src/index.js",
  "keywords": [
    "fetch",
    "javascript",
    "browser",
    "nodejs",
    "ajax"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zellwk/zl-fetch.git"
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "types": "rm src/*.d.ts; npx -p typescript tsc ./src/*.js --declaration --allowJs --emitDeclarationOnly --outDir src",
    "release": "np --no-publish && npm publish"
  },
  "author": "Zell Liew <zellwk@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@vitest/ui": "^0.23.4",
    "body-parser": "^1.20.0",
    "express": "^4.18.1",
    "form-data": "^4.0.0",
    "jsdom": "^20.0.0",
    "np": "^7.6.2",
    "portastic": "^1.0.1",
    "vite": "^3.1.3",
    "vitest": "^0.23.4"
  }
}
